{
  "name": "chat",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "npm": ">=8.11.0",
    "node": ">=16.15.0",
    "browsers": ">= 50%"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all test lint:all"
    }
  },
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:js": "npx eslint src --fix",
    "lint:css": "npx stylelint src/**/*.scss --fix",
    "prepare": "npx husky install",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "dev-start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "test": "jest",
    "start": "node server.js"
  },
  "alias": {
    "core": "./src/core",
    "pages": "./src/pages",
    "components": "./src/components",
    "utils": "./src/utils",
    "static": "./src/static",
    "hocs": "./src/hocs",
    "services": "./src/services",
    "api": "./src/api"
  },
  "keywords": [],
  "author": "Evgeniy Lapkin",
  "license": "ISC",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "^1.0.2",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@html-eslint/eslint-plugin": "^0.16.0",
    "@html-eslint/parser": "^0.16.0",
    "@parcel/transformer-sass": "^2.8.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-jest": "^29.4.1",
    "babel-loader": "^9.1.2",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "^21.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "mock-xmlhttprequest": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-import": "^15.1.0",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "express": "^4.18.2",
